			$(function(){
				$(".abbr_card li a").bind("mouseenter focus",function(){
					$(this).css({"boxShadow":"0 0 0 rgba(0,0,0,0.5)"});
				}).bind("mouseleave focusout",function(){
					$(this).css({"boxShadow":"0 0 20px rgba(0,0,0,0.5)"});
				});
			});
		</script><!-- 마우스오버 box-shadow효과 -->
		<script type="text/javascript">
			$(function(){
				// 대상 : addr_card의 li
				// 이벤트 : click
				// 이벤트 핸들러 : focus_staff의 li과 대상과 같음
				// 작은 직원카드를 클릭했을 때 포커스 창에 누른 직원카드가 떠야되고 기존에 있던 포커스 창에 직원은 숨기기
				// 클릭한 작은직원카드는 사라지고 포커스창에서 사라지는 직원이 작은 직원카드가 있던 자리에서 보이기
				$(".abbr_card li").click(function(){
					var focusNum = $(".focus_selected").attr("class").substring(11,12);
					$(this).fadeOut(50);
					$(".abbr_card li:eq("+focusNum+")").fadeIn(500);
					
					//var addrNum = $(this).find("img").attr("src").substring(17,18);
					var addrNum = $(this).find("img").attr("src").substring(24,25);
					$(".focus_staff li").removeClass("focus_selected");
					$(".focus_staff li:eq("+addrNum+")").addClass("focus_selected");
				});
			});
		</script>
